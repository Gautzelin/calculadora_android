<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button_division"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:text="/"
        tools:layout_editor_absoluteX="511dp"
        tools:layout_editor_absoluteY="220dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:text="%"
        tools:layout_editor_absoluteX="511dp"
        tools:layout_editor_absoluteY="286dp" />

    <Button
        android:id="@+id/button19"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:text="C"
        tools:layout_editor_absoluteX="419dp"
        tools:layout_editor_absoluteY="220dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:text="( )"
        tools:layout_editor_absoluteX="419dp"
        tools:layout_editor_absoluteY="286dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="120dp"
        android:layout_height="44dp"
        android:text="="
        tools:layout_editor_absoluteX="454dp"
        tools:layout_editor_absoluteY="352dp" />

    <Button
        android:id="@+id/button_suma"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:text="+"
        tools:layout_editor_absoluteX="603dp"
        tools:layout_editor_absoluteY="352dp" />

    <Button
        android:id="@+id/button_resta"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:text="-"
        tools:layout_editor_absoluteX="603dp"
        tools:layout_editor_absoluteY="284dp" />

    <Button
        android:id="@+id/button_multiplicacion"
        android:layout_width="75dp"
        android:layout_height="52dp"
        android:text="x"
        tools:layout_editor_absoluteX="603dp"
        tools:layout_editor_absoluteY="220dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout2"
        android:layout_width="729dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="64dp">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/numero1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Ingrese un numero" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="match_parent"
            android:layout_height="114dp"
            android:backgroundTint="#FAF5F5"
            android:tint="#D56CCC65"
            app:srcCompat="@drawable/ic_launcher_foreground" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/respuesta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        tools:layout_editor_absoluteX="362dp"
        tools:layout_editor_absoluteY="22dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout"
        android:layout_width="729dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="147dp">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/numero2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Ingrese un numero" />
    </com.google.android.material.textfield.TextInputLayout>

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="89dp"
        android:layout_height="137dp"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="236dp">

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Sin" />

        <RadioButton
            android:id="@+id/radioButton5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cos" />

        <RadioButton
            android:id="@+id/radioButton6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Tan" />
    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup2"
        android:layout_width="86dp"
        android:layout_height="139dp"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="237dp">

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="87dp"
            android:layout_height="wrap_content"
            android:text="Sinh" />

        <RadioButton
            android:id="@+id/radioButton4"
            android:layout_width="87dp"
            android:layout_height="wrap_content"
            android:text="Tanh" />

        <RadioButton
            android:id="@+id/radioButton3"
            android:layout_width="87dp"
            android:layout_height="wrap_content"
            android:text="Cosh" />
    </RadioGroup>

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="81dp"
        android:layout_height="30dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>